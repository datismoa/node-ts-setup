{
  "name": "node-ts-setup",
  "version": "1.0.0",
  "description": "Node TS Setup",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "watch": "nodemon --exec node --no-warnings=ExperimentalWarning --loader=ts-node/esm ./src/index.ts",
    "run:production": "node dist/index.js"
  },
  "author": "datismoa",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@swc/helpers": "^0.5.3",
    "nodemon": "^3.0.2",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=21.1.0",
    "pnpm": ">=8.10.3"
  }
}